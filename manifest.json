{
  "manifest_version": 3,
  "name": "Telivision Show Search",
  "description": "Search for your fav TV shows",
  "version": "1.0",
  "icons": {
    "16": "telivision.jpeg",
    "48": "telivision.jpeg",
    "128": "telivision.jpeg"
  },
  "action": {
    "default_icon": "telivision.jpeg",
    "default_title": "TV Search",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
